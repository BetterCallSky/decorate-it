{
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base"
  ],
  "plugins": [
    "babel"
  ],
  "env": {
    "node": true
  },
  "globals": {
  },
  "rules": {
    "no-plusplus": 0,
    "no-param-reassign": 0,
    "max-len": [2, 120],
    "generator-star-spacing": 0,
    "no-underscore-dangle": 0,
    "arrow-parens": 0,
    "babel/arrow-parens": 1
  }
}
